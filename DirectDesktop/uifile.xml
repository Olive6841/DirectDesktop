<duixml>
	<HWNDElement resid="main" layoutpos="client" layout="borderlayout()" sheet="default" background="rgb(0,0,0)">
		<Element id="atom(sampleText)" content="" layoutpos="top"/>
		<Element layout="borderlayout()" layoutpos="client" width="4096rp" height="4096rp">
			<Element id="atom(mainContainer)" layout="filllayout()" layoutpos="client" animation="alpha|s|veryfast|delayshort">
				<Element id="atom(UIContainer)" layout="filllayout()" layoutpos="absolute" background="argb(0,0,0,0)" animation="position|s|fast">

				</Element>
				<TouchButton layout="filllayout()" id="atom(prevpageMain)" layoutpos="left" class="pageselector">
					<RichText layoutpos="left" background="resbmp(3001,7,-1,0,0,1,1)" content="resstr(140)" animation="rectangle|s|fast" width="160rp"/>
				</TouchButton>
				<TouchButton layout="filllayout()" id="atom(nextpageMain)" layoutpos="right" class="pageselector">
					<RichText layoutpos="right" background="resbmp(3002,7,-1,0,0,1,1)" content="resstr(141)" animation="rectangle|s|fast" width="160rp"/>
				</TouchButton>
				<Element id="atom(selector)" class="selector" animation="alpha|s|mediumfast" layoutpos="none" background="ImmersiveControlTransparent" borderthickness="rect(1rp,1rp,1rp,1rp)" bordercolor="themeable(20005, highlight)"/>
				<Element id="atom(selector2)" class="selector" animation="alpha|s|mediumfast" layoutpos="none"/>
				<Element id="atom(dragpreview)" visible="false" layoutpos="absolute" alpha="160" layout="flowlayout(0,2,2,0)">
					<Element id="atom(multipleitems)" visible="false" alpha="255" background="themeable(dtb(DragDrop,8,0), white)" font="themeable(gtf(DragDrop,8,0), 'iconfont')" content="1"
							 foreground="themeable(gtc(DragDrop,8,0,3803), black)" padding="themeable(gtmar(DragDrop,8,0,3602), rect(5rp,1rp,5rp,1rp))" compositedtext="true" textglowsize="0"/>
				</Element>
			</Element>
		</Element>
	</HWNDElement>
	<Element resid="page" layoutpos="client" background="argb(0,0,0,0)"/>
	<Button resid="emptyspace" layoutpos="client" background="argb(0,0,0,0)">
		<Button id="atom(checkboxElem)" layoutpos="none"/> ///dummy checkbox to prevent crashing
	</Button>
	<LVItem resid="outerElem" layout="filllayout()" visible="false" layoutpos="absolute" animation="position|s|fast">
		<Element id="atom(innerElem)" layoutpos="client" class="item"/>
		<Element id="atom(iconElemShadow)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="64rp" height="64rp" alpha ="0" animation="alpha|s|mediumslow|delayshort"/>
		<DDScalableElement id="atom(iconElem)" background="argb(0,0,0,0)" contentalign="bottomcenter" layoutpos="absolute" width="48rp" height="48rp" DrawType="1"/>
		<Element id="atom(shortcutElem)" direction="ltr" background="argb(0,0,0,0)" layoutpos="absolute" alpha ="0" animation="alpha|s|mediumslow|delayshort" contentalign="bottomleft"/>
		<RichText id="atom(textElemShadow)" background="argb(0,0,0,0)" layoutpos="bottom"/>
		<RichText id="atom(textElem)" background="argb(0,0,0,0)" layoutpos="bottom" padding="rect(0,0,0,1rp)" contentalign="topcenter"/>
		<Element layout="ninegridlayout()" layoutpos="ninetopleft">
			<Button id="atom(checkboxElem)" class="checkbox" visible="false" layoutpos="ninetopleft" width="20rp" height="20rp" margin="rect(6rp,6rp,0,0)"/>
		</Element>
	</LVItem>
	<LVItem resid="outerElemGrouped" layout="filllayout()" background="argb(0,0,0,0)" layoutpos="absolute">
		<Element id="atom(innerElem)" layoutpos="client" class="item"/>
		<Element id="atom(iconElemShadow)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="64rp" height="64rp"/>
		<DDScalableElement id="atom(iconElem)" background="argb(0,0,0,0)" contentalign="bottomcenter" layoutpos="absolute" width="48rp" height="48rp" DrawType="1"/>
		<Element id="atom(shortcutElem)" direction="ltr" background="argb(0,0,0,0)" layoutpos="absolute" contentalign="bottomleft"/>
		<RichText id="atom(textElem)" background="argb(0,0,0,0)" layoutpos="bottom" contentalign="topcenter"/>
	</LVItem>
	<LVItem resid="outerElemTouch" layout="filllayout()" visible="false" layoutpos="absolute" class="itemTouchOuter" padding="rect(1rp,1rp,1rp,1rp)" width="144rp" height="144rp" animation="position|s|fast">
		<Element id="atom(innerElem)" class="itemTouchInner"/>
		<Element id="atom(containerElem)" layoutpos="client" layout="filllayout()" padding="rect(3rp,3rp,3rp,3rp)" background="argb(0,0,0,0)">
			<Element id="atom(iconcontainer)" layoutpos="bottom" layout="filllayout()" background="argb(0,0,0,0)">
				<Element id="atom(iconElemShadow)" contentalign="bottomleft" background="argb(0,0,0,0)" layoutpos="left" width="48rp" height="48rp" alpha ="0" animation="alpha|s|mediumslow|delayshort"/>
				<Element layoutpos="left" layout="borderlayout()">
					<DDScalableElement id="atom(iconElem)" background="argb(0,0,0,0)" contentalign="bottomleft" layoutpos="bottom" width="48rp" height="48rp" padding="rect(8rp,8rp,8rp,8rp)" DrawType="1"/>
				</Element>
				<Element id="atom(shortcutElem)" direction="ltr" background="argb(0,0,0,0)" contentalign="bottomleft" layoutpos="left" width="48rp" height="48rp" padding="rect(8rp,8rp,8rp,8rp)" alpha="0" animation="alpha|s|mediumslow|delayshort"/>
			</Element>
			<RichText id="atom(textElemShadow)" layoutpos="absolute" x="6rp" y="6rp" width="124rp" height="76rp" background="argb(0,0,0,0)"/>
			<RichText id="atom(textElem)" layoutpos="absolute" x="8rp" y="8rp" width="120rp" height="72rp" background="argb(0,0,0,0)"/>
		</Element>
		<Element layout="ninegridlayout()" layoutpos="ninetopright" background="argb(0,0,0,0)">
			<Button id="atom(checkboxElem)" class="checkbox" visible="false" layoutpos="ninetopright" width="20rp" height="20rp" margin="rect(0,8rp,8rp,0)"/>
		</Element>
	</LVItem>
	<Element resid="GroupedIcon" layoutpos="absolute" background="argb(0,0,0,0)" contentalign="bottomcenter"/>
	<stylesheets>
		<style resid="default">
			<if id="atom(mainContainer)">
				<Element background="black"/>
			</if>
			<if class="item">
				<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha="0" animation="alpha|log|veryfast"/>
				<if mousefocused="true">
					<Element background="themeable(dtb(Desktop::ListView,1,2), argb(0,0,0,0))" alpha="255"/>
				</if>
				<if selected="true">
					<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha ="255"/>
					<if mousefocused="true">
						<Element background="themeable(dtb(Desktop::ListView,1,6), argb(0,0,0,0))" alpha="255"/>
					</if>
				</if>
			</if>
			<if class="itemTouchOuter">
				<LVItem cursor="hand"/>
			</if>
			<if class="itemTouchInner">
				<Element layoutpos="client"/>
			</if>
			<if class="selector">
				<Element alpha="themeable(224, 148)"/>
				<if visible="false">
					<Element alpha="themeable(0, 148)"/>
				</if>
			</if>
			<if class="checkbox">
				<Button content="themeable(dtb(DirectUI::Button,3,1), '')" font="12pt;Normal;None;Marlett" padding="themeable(rect(0,0,0,0), rect(0,1rp,2rp,0))" background="themeable(argb(0,0,0,0), dfc(4, 0x0010 | 0x0200))"/>
				<if mousefocused="true">
					<Button content="themeable(dtb(DirectUI::Button,3,2), '')"/>
				</if>
				<if selected="true">
					<Button content="themeable(dtb(DirectUI::Button,3,5), 'b')"/>
					<if mousefocused="true">
						<Button content="themeable(dtb(DirectUI::Button,3,6), 'b')"/>
					</if>
				</if>
			</if>
			<if class="pageselector">
				<TouchButton background="argb(0,0,0,0)" width="16rp" foreground="white" contentalign="middlecenter" font="resstr(139)" alpha="0" animation="alpha|rectangle|s|fast"/>
				<if mousewithin="true">
					<TouchButton alpha="255" width="160rp"/>
				</if>
			</if>
			<if class="singleclicked">
				<LVItem cursor="hand"/>
			</if>
			<if class="doubleclicked">
				<LVItem cursor="arrow"/>
			</if>
			<if class="groupthumbnail">
				<DDScalableElement FirstScaledImage="101" ScaledImageIntervals="7" borderthickness="rect(8rp,8rp,8rp,8rp)" padding="rect(-8rp,-8rp,-8rp,-8rp)"/>
			</if>
		</style>
		<style resid="defaultdark">
			<if id="atom(mainContainer)">
				<Element background="black"/>
			</if>
			<if class="item">
				<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha="0" animation="alpha|log|veryfast"/>
				<if mousefocused="true">
					<Element background="themeable(dtb(Desktop::ListView,1,2), argb(0,0,0,0))" alpha="255"/>
				</if>
				<if selected="true">
					<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha ="255"/>
					<if mousefocused="true">
						<Element background="themeable(dtb(Desktop::ListView,1,6), argb(0,0,0,0))" alpha="255"/>
					</if>
				</if>
			</if>
			<if class="itemTouchOuter">
				<LVItem cursor="hand"/>
			</if>
			<if class="itemTouchInner">
				<Element layoutpos="client"/>
			</if>
			<if class="selector">
				<Element alpha="themeable(224, 148)"/>
				<if visible="false">
					<Element alpha="themeable(0, 148)"/>
				</if>
			</if>
			<if class="checkbox">
				<Button content="themeable(dtb(DirectUI::Button,3,1), '')" font="12pt;Normal;None;Marlett" padding="themeable(rect(0,0,0,0), rect(0,1rp,2rp,0))" background="themeable(argb(0,0,0,0), dfc(4, 0x0010 | 0x0200))"/>
				<if mousefocused="true">
					<Button content="themeable(dtb(DirectUI::Button,3,2), '')"/>
				</if>
				<if selected="true">
					<Button content="themeable(dtb(DirectUI::Button,3,5), 'b')"/>
					<if mousefocused="true">
						<Button content="themeable(dtb(DirectUI::Button,3,6), 'b')"/>
					</if>
				</if>
			</if>
			<if class="pageselector">
				<TouchButton background="argb(0,0,0,0)" width="16rp" foreground="white" contentalign="middlecenter" font="resstr(139)" alpha="0" animation="alpha|rectangle|s|fast"/>
				<if mousewithin="true">
					<TouchButton alpha="255" width="160rp"/>
				</if>
			</if>
			<if class="singleclicked">
				<LVItem cursor="hand"/>
			</if>
			<if class="doubleclicked">
				<LVItem cursor="arrow"/>
			</if>
			<if class="groupthumbnail">
				<DDScalableElement FirstScaledImage="201" ScaledImageIntervals="7" borderthickness="rect(8rp,8rp,8rp,8rp)" padding="rect(-8rp,-8rp,-8rp,-8rp)"/>
			</if>
		</style>
		<style resid="scrollbarstyle">
			<TouchCarouselScrollBar width="16rp" layoutpos="nineright" accessible="true" accrole="scrollbar" background="white"/>
			<TouchScrollBar accessible="true" accrole="scrollbar" background="white"/>
			<if vertical="true">
				<TouchScrollBar width="16rp" layoutpos="nineright"/>
			</if>
			<if vertical="false">
				<TouchScrollBar height="16rp" layoutpos="ninebottom"/>
			</if>
			<if class="Line">
				<TouchRepeatButton accessible="true" accrole="pushbutton" background="white" foreground="argb(250, 136, 136, 136)" font="resstr(130)"/>
				<if mousewithin="true">
					<TouchRepeatButton background="argb(0, 0, 0, 0)" foreground="argb(255, 160, 160, 160)"/>
				</if>
				<if Pressed="true">
					<TouchRepeatButton background="argb(0, 0, 0, 0)" foreground="argb(255, 136, 136, 136)"/>
				</if>
				<if id="atom(LineUp)">
					<TouchRepeatButton layoutpos="top" height="22rp"/>
				</if>
				<if id="atom(LineDown)">
					<TouchRepeatButton layoutpos="bottom" height="22rp"/>
				</if>
				<if id="atom(LineLeft)">
					<TouchRepeatButton layoutpos="left" width="22rp"/>
				</if>
				<if id="atom(LineRight)">
					<TouchRepeatButton layoutpos="right" width="22rp"/>
				</if>
			</if>
			<if class="Line_Arrow">
				<InternalRichText background="white" constrainlayout="narrow" direction="LTR" overhang="true"/>
				<if id="atom(LineUp_Arrow)">
					<InternalRichText content="resstr(131)" contentalign="bottomcenter" linespacing="33rp" baseline="27rp"/>
				</if>
				<if id="atom(LineDown_Arrow)">
					<InternalRichText content="resstr(132)" contentalign="topcenter" linespacing="33rp" baseline="17rp"/>
				</if>
				<if id="atom(LineLeft_Arrow)">
					<InternalRichText content="resstr(133)" contentalign="middleright" linespacing="17rp" baseline="14rp" padding="rect(0,0,7rp,0)"/>
				</if>
				<if id="atom(LineRight_Arrow)">
					<InternalRichText content="resstr(134)" contentalign="middleleft" linespacing="17rp" baseline="14rp" padding="rect(7rp,0,0,0)"/>
				</if>
			</if>
			<if id="atom(TouchScrollBar_Track)">
				<Element layoutpos="client" background="argb(0,0,0,0)"/>
			</if>
			<if class="Page">
				<TouchRepeatButton accessible="true" accrole="pushbutton" background="argb(0,0,0,0)"/>
				<if id="atom(PageUp)">
					<TouchRepeatButton width="16rp"/>
				</if>
				<if id="atom(PageDown)">
					<TouchRepeatButton width="16rp"/>
				</if>
				<if id="atom(PageLeft)">
					<TouchRepeatButton height="16rp"/>
				</if>
				<if id="atom(PageRight)">
					<TouchRepeatButton height="16rp"/>
				</if>
			</if>
			<if class="Thumb_Mouse">
				<Thumb accessible="true" accrole="indicator" background="argb(255, 136, 136, 136)"/>
				<if mousewithin="true">
					<Thumb background="argb(255, 160, 160, 160)" bordercolor="20768"/>
				</if>
				<if captured="true">
					<Thumb background="argb(255, 136, 136, 136)" bordercolor="20769"/>
				</if>
				<if id="atom(Thumb)">
					<Thumb width="6rp" minsize="size(0,16rp)" margin="rect(5rp,0,0,0)"/>
				</if>
				<if id="atom(HThumb)">
					<Thumb height="6rp" minsize="size(16rp,0)" margin="rect(0,5rp,0,0)"/>
				</if>
			</if>
			<if class="Thumb_Pan">
				<Thumb accessible="true" accrole="indicator" background="argb(255, 136, 136, 136)"/>
				<if id="atom(Thumb)">
					<Thumb width="2rp" minsize="size(0,16rp)" margin="rect(7rp,0,0,0)"/>
				</if>
				<if id="atom(HThumb)">
					<Thumb height="2rp" minsize="size(16rp,0)" margin="rect(0,7rp,0,0)"/>
				</if>
			</if>
			<if class="ScrollViewer">
				<CCHScrollBar layoutpos="ninebottom" accessible="true" accdesc="resstr(1101)" accname="resstr(1102)" />
				<CCVScrollBar layoutpos="nineright" accessible="true" accdesc="resstr(1100)" accname="resstr(1103)" />
				<viewer layoutpos="nineclient" />
			</if>
		</style>
	</stylesheets>
</duixml>