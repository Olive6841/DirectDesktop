<duixml>
	<HWNDElement resid="main" layoutpos="client" layout="borderlayout()" sheet="default">
		<Element id="atom(sampleText)" content="" layoutpos="top"/>
		<Element layout="borderlayout()" layoutpos="client">
			<Element id="atom(mainContainer)" layout="filllayout()" layoutpos="client" background="gradient(lightblue, lightsteelblue, 1)">
				<Element id="atom(UIContainer)" layout="filllayout()" layoutpos="client" background="argb(0,0,0,0)">

				</Element>
				<Element id="atom(selector)" class="selector" layoutpos="none" animation="alpha|s|mediumfast" background="themeable(20066, hotlight)" borderthickness="rect(1rp,1rp,1rp,1rp)" bordercolor="themeable(20005, highlight)"/>
				<Element id="atom(fullscreenpopup)" layout="filllayout()" layoutpos="none" background="argb(0,0,0,0)" margin="rect(0,0,0,0)" alpha="0">
					<Button id="atom(fullscreenpopupbase)" layout="flowlayout(0,2,2,0)" layoutpos="client" class="popupbg" visible="false">
						<Element id="atom(centered)" layout="filllayout()" background="argb(0,0,0,0)"/>
					</Button>
					<Element id="atom(tools)" layout="flowlayout(0,0,2,0)" layoutpos="none" background="Window" padding="rect(8rp,8rp,8rp,8rp)">
						<CCPushButton id="atom(testButton5)" height="32rp" width="0" content="Initialize layout"/>
						<CCPushButton id="atom(testButton)" height="32rp" width="128rp" content="Refresh" Enabled="false"/>
						<CCPushButton id="atom(testButton3)" height="32rp" width="0" content="Simulate popup"/>
						<CCPushButton id="atom(testButton4)" height="32rp" width="128rp" content="AccentColorIcons (N)"/>
						<CCPushButton id="atom(testButton2)" height="32rp" width="128rp" content="TreatDirAsGroup (N)"/>
						<Element id="atom(itemcountstatus)" animation="alpha|s|slow|delaylong" foreground="themeable(gtc(ControlPanel,7,1,3803), hotlight)" font="messagefont;115%"/>
					</Element>
				</Element>
			</Element>
		</Element>
	</HWNDElement>
	<Button resid="emptyspace" layoutpos="client" background="argb(0,0,0,0)">
		<Button id="atom(checkboxElem)" layoutpos="none"/> ///dummy checkbox to prevent crashing
	</Button>
	<Button resid="outerElem" layout="filllayout()" sheet="default" visible="false" layoutpos="absolute" alpha="255" animation="alpha|s|fast">
		<Element id="atom(innerElem)" layoutpos="client" class="item"/>
		<Element id="atom(iconElemShadow)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="64rp" height="64rp" alpha ="0" animation="alpha|s|mediumslow|delayshort"/>
		<Element id="atom(iconElem)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="48rp" height="48rp"/>
		<Element id="atom(shortcutElem)" background="argb(0,0,0,0)" layoutpos="absolute" width="32rp" height="32rp" alpha ="0" animation="alpha|s|mediumslow|delayshort"/>
		<RichText id="atom(textElemShadow)" background="argb(0,0,0,0)" layoutpos="bottom"/>
		<RichText id="atom(textElem)" background="argb(0,0,0,0)" layoutpos="bottom" padding="rect(0,0,0,1rp)"/>
		<Button id="atom(checkboxElem)" class="checkbox" visible="false" layoutpos="absolute" width="20rp" height="20rp" x="6rp" y="6rp"/>
	</Button>
	<Button resid="outerElemGrouped" layout="filllayout()" sheet="default" background="argb(0,0,0,0)" layoutpos="absolute" alpha="255" animation="alpha|s|fast">
		<Element id="atom(innerElem)" layoutpos="client" class="item"/>
		<Element id="atom(iconElemShadow)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="64rp" height="64rp"/>
		<Element id="atom(iconElem)" contentalign="bottomcenter" background="argb(0,0,0,0)" layoutpos="absolute" width="48rp" height="48rp"/>
		<Element id="atom(shortcutElem)" background="argb(0,0,0,0)" layoutpos="absolute" width="32rp" height="32rp"/>
		<RichText id="atom(textElem)" background="argb(0,0,0,0)" layoutpos="bottom"/>
	</Button>
	<Button resid="fullscreeninner" class="popupwindow" layoutpos="absolute" layout="filllayout()"/>
	<Element resid="groupdirectory" layoutpos="client" layout="borderlayout()" padding="rect(16rp,0,16rp,16rp)" background="argb(0,0,0,0)">
		<Element id="atom(dirtitle)" layout="borderlayout()" layoutpos="top" padding="rect(0,16rp,0,16rp)">
			<Element id="atom(tasksanimator)" layoutpos="right"/>
			<Element id="atom(dirnameanimator)" layoutpos="left"/>
			<Element id="atom(tasks)" layoutpos="right" layout="gridlayout(1,2)" font="iconfont">
				<TouchButton id="atom(Customize)" content="Customize group" contentalign="middlecenter" class="popupbutton"/>
				<TouchButton id="atom(OpenInExplorer)" content="Open in File Explorer" contentalign="middlecenter" class="popupbutton"/>
			</Element>
			<RichText id="atom(dirname)" class="popuptext" layoutpos="client" padding="rect(0,0,0,2rp)" contentalign="middleleft" font="iconfont;160%" alpha="0" animation="alpha|linear|mediumfast|delaymedium"/>
		</Element>
		<ScrollViewer id="atom(groupdirlist)" height="1rp" layoutpos="client" xscrollable="false" yscrollable="true" xbarvisibility="never" ybarvisibility="asneeded">
			<RichText id="atom(SubUIContainer)" visible="true" layoutpos="client" layout="filllayout()" class="popuptext" font="iconfont"/>
		</ScrollViewer>
	</Element>
	<stylesheets>
		<style resid="default">
			<if class="item">
				<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha="0" animation="alpha|log|veryfast"/>
				<if mousefocused="true">
					<Element background="themeable(dtb(Desktop::ListView,1,2), argb(0,0,0,0))" alpha="255"/>
				</if>
				<if selected="true">
					<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha ="255"/>
					<if mousefocused="true">
						<Element background="themeable(dtb(Desktop::ListView,1,6), argb(0,0,0,0))" alpha="255"/>
					</if>
				</if>
			</if>
			<if class="selector">
				<Element alpha="themeable(224, 148)"/>
				<if visible="false">
					<Element alpha="themeable(0, 148)"/>
				</if>
			</if>
			<if class="checkbox">
				<Button content="themeable(dtb(DirectUI::Button,3,1), '')" font="12pt;Normal;None;Marlett" padding="themeable(rect(0,0,0,0), rect(0,1rp,2rp,0))" background="themeable(argb(0,0,0,0), dfc(4, 0x0010 | 0x0200))"/>
				<if mousefocused="true">
					<Button content="themeable(dtb(DirectUI::Button,3,2), '')"/>
				</if>
				<if selected="true">
					<Button content="themeable(dtb(DirectUI::Button,3,5), 'b')"/>
					<if mousefocused="true">
						<Button content="themeable(dtb(DirectUI::Button,3,6), 'b')"/>
					</if>
				</if>
			</if>
			<if class="popupbg">
				<Button background="argb(0,0,0,0)"/>
			</if>
			<if class="popupwindow">
				<Button background="argb(224,240,240,240)"/>
			</if>
			<if class="popuptext">
				<RichText foreground="black"/>
			</if>
			<if class="popupbutton">
				<TouchButton foreground="20007"/>
			</if>
			<if class="ScrollViewer">
				<CCHScrollBar layoutpos="ninebottom" accessible="true" accdesc="resstr(1101)" accname="resstr(1102)" />
				<CCVScrollBar layoutpos="nineright" accessible="true" accdesc="resstr(1100)" accname="resstr(1103)" />
				<viewer layoutpos="nineclient" />
			</if>
			<if class="singleclicked">
				<Button cursor="hand"/>
			</if>
			<if class="doubleclicked">
				<Button cursor="arrow"/>
			</if>
		</style>
		<style resid="defaultdark">
			<if class="item">
				<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha="0" animation="alpha|log|veryfast"/>
				<if mousefocused="true">
					<Element background="themeable(dtb(Desktop::ListView,1,2), argb(0,0,0,0))" alpha="255"/>
				</if>
				<if selected="true">
					<Element background="themeable(dtb(Desktop::ListView,1,3), argb(0,0,0,0))" alpha ="255"/>
					<if mousefocused="true">
						<Element background="themeable(dtb(Desktop::ListView,1,6), argb(0,0,0,0))" alpha="255"/>
					</if>
				</if>
			</if>
			<if class="selector">
				<Element alpha="themeable(224, 148)"/>
				<if visible="false">
					<Element alpha="themeable(0, 148)"/>
				</if>
			</if>
			<if class="checkbox">
				<Button content="themeable(dtb(DirectUI::Button,3,1), '')" font="12pt;Normal;None;Marlett" padding="themeable(rect(0,0,0,0), rect(0,1rp,2rp,0))" background="themeable(argb(0,0,0,0), dfc(4, 0x0010 | 0x0200))"/>
				<if mousefocused="true">
					<Button content="themeable(dtb(DirectUI::Button,3,2), '')"/>
				</if>
				<if selected="true">
					<Button content="themeable(dtb(DirectUI::Button,3,5), 'b')"/>
					<if mousefocused="true">
						<Button content="themeable(dtb(DirectUI::Button,3,6), 'b')"/>
					</if>
				</if>
			</if>
			<if class="popupbg">
				<Button background="argb(96,32,32,32)"/>
			</if>
			<if class="popupwindow">
				<Button background="argb(224,40,40,40)"/>
			</if>
			<if class="popuptext">
				<RichText foreground="white"/>
			</if>
			<if class="popupbutton">
				<TouchButton foreground="20002"/>
			</if>
			<if class="ScrollViewer">
				<CCHScrollBar layoutpos="ninebottom" accessible="true" accdesc="resstr(1101)" accname="resstr(1102)" />
				<CCVScrollBar layoutpos="nineright" accessible="true" accdesc="resstr(1100)" accname="resstr(1103)" />
				<viewer layoutpos="nineclient" />
			</if>
			<if class="singleclicked">
				<Button cursor="hand"/>
			</if>
			<if class="doubleclicked">
				<Button cursor="arrow"/>
			</if>
		</style>
		<style resid="scrollbarstyle">
			<TouchCarouselScrollBar width="16rp" layoutpos="nineright" accessible="true" accrole="scrollbar" background="white"/>
			<TouchScrollBar accessible="true" accrole="scrollbar" background="white"/>
			<if vertical="true">
				<TouchScrollBar width="16rp" layoutpos="nineright"/>
			</if>
			<if vertical="false">
				<TouchScrollBar height="16rp" layoutpos="ninebottom"/>
			</if>
			<if class="Line">
				<TouchRepeatButton accessible="true" accrole="pushbutton" background="white" foreground="argb(250, 136, 136, 136)" font="resstr(130)"/>
				<if mousewithin="true">
					<TouchRepeatButton background="argb(0, 0, 0, 0)" foreground="argb(255, 160, 160, 160)"/>
				</if>
				<if Pressed="true">
					<TouchRepeatButton background="argb(0, 0, 0, 0)" foreground="argb(255, 136, 136, 136)"/>
				</if>
				<if id="atom(LineUp)">
					<TouchRepeatButton layoutpos="top" height="22rp"/>
				</if>
				<if id="atom(LineDown)">
					<TouchRepeatButton layoutpos="bottom" height="22rp"/>
				</if>
				<if id="atom(LineLeft)">
					<TouchRepeatButton layoutpos="left" width="22rp"/>
				</if>
				<if id="atom(LineRight)">
					<TouchRepeatButton layoutpos="right" width="22rp"/>
				</if>
			</if>
			<if class="Line_Arrow">
				<InternalRichText background="white" constrainlayout="narrow" direction="LTR" overhang="true"/>
				<if id="atom(LineUp_Arrow)">
					<InternalRichText content="resstr(131)" contentalign="bottomcenter" linespacing="33rp" baseline="27rp"/>
				</if>
				<if id="atom(LineDown_Arrow)">
					<InternalRichText content="resstr(132)" contentalign="topcenter" linespacing="33rp" baseline="17rp"/>
				</if>
				<if id="atom(LineLeft_Arrow)">
					<InternalRichText content="resstr(133)" contentalign="middleright" linespacing="17rp" baseline="14rp" padding="rect(0,0,7rp,0)"/>
				</if>
				<if id="atom(LineRight_Arrow)">
					<InternalRichText content="resstr(134)" contentalign="middleleft" linespacing="17rp" baseline="14rp" padding="rect(7rp,0,0,0)"/>
				</if>
			</if>
			<if id="atom(TouchScrollBar_Track)">
				<Element layoutpos="client" background="20575"/>
			</if>
			<if class="Page">
				<TouchRepeatButton accessible="true" accrole="pushbutton" background="20575"/>
				<if id="atom(PageUp)">
					<TouchRepeatButton width="16rp"/>
				</if>
				<if id="atom(PageDown)">
					<TouchRepeatButton width="16rp"/>
				</if>
				<if id="atom(PageLeft)">
					<TouchRepeatButton height="16rp"/>
				</if>
				<if id="atom(PageRight)">
					<TouchRepeatButton height="16rp"/>
				</if>
			</if>
			<if class="Thumb_Mouse">
				<Thumb accessible="true" accrole="indicator" background="argb(255, 136, 136, 136)"/>
				<if mousewithin="true">
					<Thumb background="argb(255, 160, 160, 160)" bordercolor="20768"/>
				</if>
				<if captured="true">
					<Thumb background="argb(255, 136, 136, 136)" bordercolor="20769"/>
				</if>
				<if id="atom(Thumb)">
					<Thumb width="6rp" minsize="size(0,16rp)" margin="rect(5rp,0,0,0)"/>
				</if>
				<if id="atom(HThumb)">
					<Thumb height="6rp" minsize="size(16rp,0)" margin="rect(0,5rp,0,0)"/>
				</if>
			</if>
			<if class="Thumb_Pan">
				<Thumb accessible="true" accrole="indicator" background="argb(255, 136, 136, 136)"/>
				<if id="atom(Thumb)">
					<Thumb width="2rp" minsize="size(0,16rp)" margin="rect(7rp,0,0,0)"/>
				</if>
				<if id="atom(HThumb)">
					<Thumb height="2rp" minsize="size(16rp,0)" margin="rect(0,7rp,0,0)"/>
				</if>
			</if>
			<if class="ScrollViewer">
				<CCHScrollBar layoutpos="ninebottom" accessible="true" accdesc="resstr(1101)" accname="resstr(1102)" />
				<CCVScrollBar layoutpos="nineright" accessible="true" accdesc="resstr(1100)" accname="resstr(1103)" />
				<viewer layoutpos="nineclient" />
			</if>
		</style>
	</stylesheets>
</duixml>